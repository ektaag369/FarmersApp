<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Seller.DashboardFragments.SellerHomeFragment">

        <TextView
            android:id="@+id/label_new_orders"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="New Orders"
            android:textSize="18sp"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:layout_marginTop="20dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginLeft="20dp"/>
        <TextView
            android:id="@+id/txt_new_orders"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="9"
            android:textSize="18sp"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:layout_marginTop="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginRight="20dp"/>
        <LinearLayout
            android:id="@+id/lin_new_orders"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:orientation="horizontal"
            android:background="@color/grey"
            app:layout_constraintTop_toBottomOf="@+id/label_new_orders"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_margin="20dp"/>
        <TextView
            android:id="@+id/label_past_orders"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Past Orders"
            android:textSize="18sp"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:layout_marginTop="20dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/lin_new_orders"
            android:layout_marginLeft="20dp"/>
        <TextView
            android:id="@+id/txt_past_orders"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="25"
            android:textSize="18sp"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:layout_marginTop="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/lin_new_orders"
            android:layout_marginRight="20dp"/>
        <LinearLayout
            android:id="@+id/lin_past_orders"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:orientation="horizontal"
            android:background="@color/grey"
            app:layout_constraintTop_toBottomOf="@+id/label_past_orders"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_margin="20dp"/>
        <TextView
            android:id="@+id/label_statistics"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Statistics"
            android:textSize="18sp"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:layout_marginTop="20dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/lin_past_orders"
            android:layout_marginLeft="20dp"/>

        <LinearLayout
            android:id="@+id/lin_statistics"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:orientation="horizontal"
            android:background="@color/grey"
            app:layout_constraintTop_toBottomOf="@+id/label_statistics"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_margin="20dp"/>

        <TextView
            android:id="@+id/label_money_withdrawal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Money Withdrawal"
            android:textSize="18sp"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:layout_marginTop="20dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/lin_statistics"
            android:layout_marginLeft="20dp"/>

        <LinearLayout
            android:id="@+id/lin_money_withdrawal"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:orientation="horizontal"
            android:background="@color/grey"
            app:layout_constraintTop_toBottomOf="@+id/label_money_withdrawal"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_margin="20dp"/>
        <TextView
            android:id="@+id/label_edit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Edit my Products"
            android:textSize="18sp"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:layout_marginTop="20dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/lin_money_withdrawal"
            android:layout_marginLeft="20dp"/>

        <LinearLayout
            android:id="@+id/lin_edit"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:orientation="horizontal"
            android:background="@color/grey"
            app:layout_constraintTop_toBottomOf="@+id/label_edit"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_margin="20dp"/>
        <TextView
            android:id="@+id/label_profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Profile Settings"
            android:textSize="18sp"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:layout_marginTop="20dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/lin_edit"
            android:layout_marginLeft="20dp"/>

        <LinearLayout
            android:id="@+id/lin_profile"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:orientation="horizontal"
            android:background="@color/grey"
            app:layout_constraintTop_toBottomOf="@+id/label_profile"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_margin="20dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>



</layout>